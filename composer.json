{
    "name": "combindma/recaptcha",
    "description": "Simple and painless Google reCAPTCHA package for Laravel framework",
    "keywords": [
        "combindma",
        "recaptcha"
    ],
    "homepage": "https://github.com/combindma/recaptcha",
    "license": "MIT",
    "authors": [
        {
            "name": "Combind",
            "email": "hello@combind.ma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/sail": "^1.4",
        "orchestra/testbench": "^6.13",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Combindma\\Recaptcha\\": "src",
            "Combindma\\Recaptcha\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Combindma\\Recaptcha\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Combindma\\Recaptcha\\RecaptchaServiceProvider"
            ],
            "aliases": {
                "Recaptcha": "Combindma\\Recaptcha\\RecaptchaFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
